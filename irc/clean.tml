~system(?x)				:- system(?x).
~system(?x ?y)				:- system(?x ?y).
~system(?x ?y ?z)			:- system(?x ?y ?z).
~system(?x ?y ?z ?a)			:- system(?x ?y ?z ?a).
~system(?x ?y ?z ?a ?b)			:- system(?x ?y ?z ?a ?b).
~system(?x ?y ?z ?a ?b ?c)		:- system(?x ?y ?z ?a ?b ?c).
~system(?x ?y ?z ?a ?b ?c ?d)		:- system(?x ?y ?z ?a ?b ?c ?d).
~system(?x ?y ?z ?a ?b ?c ?d ?e)	:- system(?x ?y ?z ?a ?b ?c ?d ?e).
~clear(?x)				:- clear(?x).
~fileout_rule(?x)			:- fileout_rule(?x).
